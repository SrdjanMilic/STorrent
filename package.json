  "config": {
    "forge": {
      "packagerConfig": {
        "packageName": "STorrent"
      },
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "icon": "icons/win/icon.ico",
        "packageManager": "npm",
        "ignore": [
          ".vscode",
          "assets",
          "out",
          "screenshots",
          "notes"
        ]
      },
      "electronInstallerDebian": {
        "icon": "icons/png/256x256.png"
      },
      "electronWinstallerConfig": {
        "name": "STorrent"
      },
      "github_repository": {
        "owner": "SrdjanMilic",
        "name": "https://github.com/SrdjanMilic/STorrent"
      },
      "windowsStoreConfig": {
        "packageName": "STorrent",
        "name": "STorrent"
      }
    }
